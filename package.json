    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@semantic-release/github": "5.2.10",
    "commitizen": "3.1.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "gatsby-source-filesystem": "2.0.33",
    "husky": "2.1.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "sort-package-json": "1.22.1"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
