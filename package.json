{
  "name": "noah-blon-dot-com",
  "version": "0.0.0",
  "private": true,
  "description": "My Homepage.",
  "keywords": [
    "blon",
    "noah",
    "noahblon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/noahblon/noah-blon-dot-com.git"
  },
  "license": "MIT",
  "author": "Noah Blon <noahblon@gmail.com> (http://www.noahblon.com)",
  "scripts": {
    "build": "npx gatsby build",
    "commit": "npx git-cz",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground npx gatsby develop",
    "serve": "npx gatsby serve",
    "start": "npm run develop",
    "test": "npm run test:jest",
    "test:coverage": "jest --coverage",
    "test:jest": "jest",
    "test:watch": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "1.0.16",
    "@mdx-js/react": "1.0.16",
