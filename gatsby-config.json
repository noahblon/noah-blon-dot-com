{
  "siteMetadata": {
    "title": "Noah Blon's Homepage",
    "siteUrl": "https://noahblon.com",
    "description": "Noah Blon's awesome homepage."
  },
  "plugins": [
    {
      "resolve": "gatsby-mdx",
      "options": {
        "defaultLayouts": {
          "default": "/Users/nblon/Sites/noah-blon-dot-com/src/components/layout.js"
        }
      }
    },
    "gatsby-plugin-react-helmet",
    "gatsby-plugin-postcss",
    {
      "resolve": "gatsby-plugin-sharp",
      "options": {
        "useMozJpeg": false,
        "stripMetadata": true,
        "defaultQuality": 75
      }
    },
    {
      "resolve": "gatsby-source-filesystem",
      "options": {
        "name": "posts",
        "path": "src/posts"
      }
    },
    "gatsby-transformer-sharp"
  ]
}
